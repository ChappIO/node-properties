{
  "name": "node-properties",
  "version": "0.2.1-0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChappIO/node-properties.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChappIO/node-properties/issues"
  },
  "homepage": "https://github.com/ChappIO/node-properties#readme",
  "dependencies": {
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^11.13.6",
    "esdoc": "^1.1.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "tabWidth": 2
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "excludes": [
      "explode.ts"
    ],
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-typescript-plugin"
      },
      {
        "name": "esdoc-importpath-plugin",
        "option": {
          "stripPackageName": false,
          "replaces": [
            {
              "from": ".*",
              "to": ""
            }
          ]
        }
      }
    ]
  }
}
